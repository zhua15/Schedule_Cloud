<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zhua15.schedulecloud">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.zhua15.schedulecloud.LoginActivity"
            android:label="@string/app_name">
        </activity>
        <activity
            android:name="com.zhua15.schedulecloud.SignupActivity"
            android:label="@string/app_name">
        </activity>
        <activity
            android:name="com.zhua15.schedulecloud.patientPop"
            android:label="@string/app_name"
            android:theme="@style/CustomTheme">
        </activity>
        <activity
            android:name="com.zhua15.schedulecloud.doctorPop"
            android:label="@string/app_name"
            android:theme="@style/CustomTheme">
        </activity>
        <activity
            android:name="com.zhua15.schedulecloud.TableActivity"
            android:label="@string/app_name">
        </activity>
        <activity
            android:name="com.zhua15.schedulecloud.delayPop"
            android:label="@string/app_name"
            android:theme="@style/CustomTheme">
        </activity>
    </application>

</manifest>